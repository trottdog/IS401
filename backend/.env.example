# Copy this file to .env and fill in your values.
# Do not commit .env (it should be in .gitignore).

# PostgreSQL connection string (required for DB APIs and drizzle-kit).
# Format: postgresql://USER:PASSWORD@HOST:PORT/DATABASE
DATABASE_URL=postgresql://postgres:password@localhost:5432/byuconnect

# Optional. Port for the API server (default: 5000).
PORT=5000
